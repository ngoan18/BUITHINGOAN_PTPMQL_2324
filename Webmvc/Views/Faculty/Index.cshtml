@model IEnumerable<Webmvc.Models.Faculty>
    <p>
        <a class="btn btn-success" asp-action="Create"> Creat a new Faculty</a>
    </p>
    <table class="table table-all">
        <tr>
            <th>FacultyID</th>
            <th>FacultyName</th>
        </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.FacultyID)</td>
            <td>@Html.DisplayFor(m => item.FacultyName)</td>
        </tr>
    }

    </table>