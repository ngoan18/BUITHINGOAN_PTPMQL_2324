@model Webmvc.Models.Faculty

@{

    ViewData["Title"]="Delete";

}

<h3>Bạn có muốn xóa thông tin Khoa khong </h3>

<div>
    <hr />
    <dl class = "row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FacultyName):
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FacultyName)
        </dd>
    </dl>
    <form asp-action="Delete">
        <input type="hidden" asp-for="FacultyID"/>
        <input type="submit" value="Delete" class="btn btn-danger" />|
        <a class="btn btn-success" asp-action="Index">Danh sách khoa</a>
    </form>

</div>